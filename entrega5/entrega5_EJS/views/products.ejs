<body>
  <main>
    <h1>Lista de todos los productos</h1>
    <ul>
      <% for(var i = 0; i < products.length; i++) {
          <li><%= products[i].thumbnail %></li>
        <li><%= products[i].title %></li>
        <li><%= products[i].price %></li>
      <% } %>

    </ul>
  </main>
</body>

</html>
